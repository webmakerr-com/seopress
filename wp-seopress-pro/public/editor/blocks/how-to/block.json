{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wpseopress/how-to",
  "title": "How To",
  "description": "Display a tutorial block",
  "icon": "feedback",
  "category": "wpseopress",
  "textdomain": "wp-seopress-pro",
  "supports": {
    "align": true,
    "html": false,
    "color": {
      "gradients": true,
      "link": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    }
  },
  "attributes": {
    "description": {
      "type": "string",
      "default": ""
    },
    "unorderedList": {
      "type": "boolean",
      "default": false
    },
    "estimatedCost": {
      "type": "string",
      "default": ""
    },
    "durationDays": {
      "type": "string",
      "default": ""
    },
    "durationHours": {
      "type": "string",
      "default": ""
    },
    "durationMinutes": {
      "type": "string",
      "default": ""
    },
    "schema": {
      "type": "object",
      "default": {}
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
}